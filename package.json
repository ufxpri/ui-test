{
  "name": "electron-react-vite-app",
  "version": "1.0.0",
  "main": "electron/main.js",
  "private": true,
  "scripts": {
    "dev": "concurrently \"vite\" \"npm run electron:dev\"",
    "electron:dev": "wait-on http://localhost:5173 && cross-env NODE_ENV=development electron electron/main.js",
    "build": "vite build",
    "preview": "vite preview",
    "postinstall": "electron-rebuild",
    "generate:types": "openapi-typescript api-schema/openapi.yaml -o api-schema/generated/typescript/schema.d.ts",
    "backend:dev": "cd backend && python -m app.main",
    "backend:install": "cd backend && pip install -r requirements.txt"
  },
  "dependencies": {
    "better-sqlite3": "^12.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.0.0",
    "concurrently": "^8.2.0",
    "cross-env": "^7.0.3",
    "electron": "^29.0.0",
    "electron-rebuild": "^3.2.9",
    "openapi-typescript": "^6.7.3",
    "vite": "^5.0.0",
    "wait-on": "^7.0.1"
  }
}
